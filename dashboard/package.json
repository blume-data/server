{
  "name": "ranjod-dashboard",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@minimal_ui/save_data": "^1.0.21",
    "@ranjodhbirkaur/constants": "^1.0.137",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@tinymce/tinymce-react": "^3.10.1",
    "@types/jest": "^26.0.15",
    "@types/luxon": "^1.25.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-thunk": "^2.1.0",
    "axios": "^0.21.4",
    "date-fns": "^2.0.0-beta.5",
    "imagekit-javascript": "^1.3.6",
    "luxon": "^1.25.0",
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "redux-thunk": "^2.3.0",
    "sass": "^1.32.13",
    "shrink-ray-current": "^4.1.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start:dev": "react-scripts start",
    "dev": "REACT_APP_ENV=development npm run start:dev",
    "start:local": "REACT_APP_ENV=test npm run start:dev",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:prod": "npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:server": "node server/index.js",
    "deploy": "export HTTPS=true &&npm run start:server",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/node-logger": "^6.3.2",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.4.18",
    "source-map-explorer": "^2.5.2"
  }
}
