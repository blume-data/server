{
  "name": "ranjod-dashboard",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.3",
    "@ranjodhbirkaur/constants": "^1.0.137",
    "@tinymce/tinymce-react": "^4.3.0",
    "@types/jest": "^26.0.15",
    "@types/luxon": "^1.25.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-thunk": "^2.1.0",
    "axios": "^0.21.4",
    "date-fns": "^2.0.0-beta.5",
    "imagekit-javascript": "^1.3.6",
    "luxon": "^1.25.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1",
    "redux-thunk": "^2.3.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start:dev": "react-scripts --openssl-legacy-provider start",
    "dev": "REACT_APP_ENV=development npm run start:dev",
    "start:local": "REACT_APP_ENV=test npm run start:dev",
    "build:prod": "GENERATE_SOURCEMAP=false react-scripts build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:server": "node server/index.js",
    "deploy": "export HTTPS=true &&npm run start:server",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss-flexbugs-fixes": "^5.0.2",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
