{
  "name": "auth",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run start:server",
    "start:server": "node build/index.js",
    "dev": "ts-node-dev --poll src/index.ts",
    "test": "jest --watchAll --no-cache",
    "test:ci": "jest",
    "build": "webpack",
    "ingress": "kubectl apply -f https://raw.githubusercontent.com/kubernetes/ingress-nginx/master/deploy/static/provider/cloud/deploy.yaml",
    "clear-pods": "kubectl delete deployment auth-depl && kubectl delete deployment auth-mongo-depl",
    "jwt": "kubectl create secret generic jwt-secret --from-literal=JWT_KEY=sdjfksdjfdskfjdsk",
    "DO:pods": "kubectl --kubeconfig=\".kube/api-cluster-kubeconfig.yaml\" get pods",
    "update-@ranjodhbirkaur/common": "npm update @ranjodhbirkaur/common",
    "update-@ranjodhbirkaur/constants": "npm update @ranjodhbirkaur/constants"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/test/setup.ts"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ranjodhbirkaur/common": "^1.0.74",
    "@ranjodhbirkaur/constants": "^1.0.74",
    "@types/cookie-session": "2.0.39",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mongoose": "^5.7.8",
    "@types/supertest": "^2.0.8",
    "axios": "^0.20.0",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.4.0",
    "jest": "^25.2.7",
    "jsonwebtoken": "^8.5.1",
    "mongodb-memory-server": "^6.5.1",
    "mongoose": "^5.9.7",
    "supertest": "^4.0.2",
    "ts-jest": "^25.3.1",
    "ts-loader": "^7.0.5",
    "ts-node-dev": "^1.0.0-pre.49",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  }
}
